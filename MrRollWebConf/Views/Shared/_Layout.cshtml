<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@TempData["Title"] - MrRollWebConf</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <link rel="stylesheet" href="~/lib/jqueryui/jquery-ui.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/4.1.0/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/lib/jqueryui/jquery-ui.min.css" />
    </environment>

</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top menu">
        <div class="container">
            <div class="brand">
                <a class="brand" asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">MrRollWebConf</a>
            </div>
            <div>
                <a class="menu-item" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                <a class="menu-item" asp-area="" asp-controller="Categories" asp-action="Index">Categories</a>
                <a class="menu-item" asp-area="" asp-controller="Users" asp-action="Index">Users</a>
                <a class="menu-item" asp-area="" asp-controller="Home" asp-action="Description">Description</a>
                <a class="menu-item" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
                @*@await Html.PartialAsync("_LoginPartial")*@
            </div>
        </div>
    </nav>
    <div class="container body-content">

        @if (TempData["success"] != null)
        {
            <br /><div class="alert alert-success" role="alert">@TempData["success"]</div>
        }
        @if (TempData["error"] != null)
        {
            <br /><div class="alert alert-danger" role="alert">@TempData["error"]</div>
        }
        @if (TempData["warning"] != null)
        {
            <br /><div class="alert alert-warning" role="alert">@TempData["warning"]</div>
        }
        @if (TempData["info"] != null)
        {
            <br /><div class="alert alert-info" role="alert">@TempData["info"]</div>
        }






        @RenderBody()

        <footer>
            <div class="navbar navbar-inverse navbar-fixed-bottom">
                <p class="container">&copy; 2018 - MrRollWebConf</p>
            </div>
        </footer>
    </div>

    <script src="~/lib/popper.js/dist/popper.js"></script>

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/jqueryui/jquery-ui.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-K+ctZQ+LL8q6tP7I94W+qzQsfRV2a+AfHIi9k8z8l9ggpc8X+Ytst4yBo/hH+8Fk">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/4.1.0/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
